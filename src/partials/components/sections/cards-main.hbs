            <div class="ordinary-slider swiper loading">
              <div class="ordinary-slider__swiper swiper-wrapper">
                <div class="ordinary-slider__slide swiper-slide">
                  <div class="ordinary-slider__slide-box">
                    <img class="ordinary-slider__slide-box-img" src="img/content/cards-main/1.jpg" alt="Лаборатория">
                    <ul class="ordinary-slider__slide-box-list">
                      <li>
                        Разработка под заказчика
                      </li>
                    </ul>
                  </div>
                  <div class="ordinary-slider__slide-wrapper">
                    <div class="ordinary-slider__slide-logo">
                      <img class="ordinary-slider__slide-logo-img" src="img/content/cards-main/logo-company.png" alt="Сбер">
                    </div>
                    <div class="ordinary-slider__slide-content">
                      <ul class="ordinary-slider__slide-content-header">
                        <li class="ordinary-slider__slide-content-header-item">
                          Разработка
                        </li>
                        <li class="ordinary-slider__slide-content-header-item">
                          2021
                        </li>
                      </ul>
                      <a class="ordinary-slider__slide-content-title" href="#">
                        Разработка личного кабинета для Сбербанка
                      </a>
                      {{> components/user-card}}
